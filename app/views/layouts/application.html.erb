<!DOCTYPE html>
<html>
  <head>
    <title>Ingh</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= render 'layouts/fonts' %>

  </head>

  <body>

   <div class="layout-container navgap">

        <%= render '/layouts/nav' %>


     

       <% if notice.present? %>
       <div class="alert alert-success fade in text-center">
                            <span class="close" data-dismiss="alert"><em class="ion-close" style="font-size:24px; color: #ff0000;"></em></span>
                            <i class="fa fa-check fa-2x pull-left"></i>
                            <p class="text-center" style="font-size: 20px; color: #ffffff;"><%= notice %></p>
                        </div>
            <% else %>

            <% end %>

    <%= yield %>

    <span class="card"><%= alert %></span>
         <%# content_for :script do %>
             
              <%# new app.Urban_terms;%>

          <%# end %>

<script>
  $(function() {
    <%= yield :script %>
  });
</script>


    </div>


