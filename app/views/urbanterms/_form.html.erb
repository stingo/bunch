<%= simple_form_for(@urbanterm) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :meaning1 %>
    <%= f.input :meaning2 %>
    <%= f.input :origin %>
    <%= f.input :example1 %>
    <%= f.input :example2 %>
    <%= f.input :example3 %>
    <%= f.input :example4 %>
    <%= f.input :termcover %>
    <%= f.input :termaudio %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
