<p id="notice"><%= notice %></p>

<h1>Companytypes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companytypes.each do |companytype| %>
      <tr>
        <td><%= companytype.name %></td>
        <td><%= companytype.desc %></td>
        <td><%= link_to 'Show', companytype %></td>
        <td><%= link_to 'Edit', edit_companytype_path(companytype) %></td>
        <td><%= link_to 'Destroy', companytype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Companytype', new_companytype_path %>
